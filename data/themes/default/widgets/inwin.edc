group { name: "elm/win/inwin/modal_window";
   parts {
      part { name: "block_events";
         type: RECT;
         scale: 1;
         description { state: "default" 0.0;
            color: 0 0 0 0;
         }
         description { state: "clicked" 0.0;
            color: 255 0 0 50;
         }
      }
      part { name: "pop";
         type: IMAGE;
         scale: 1;
         description { state: "default" 0.0;
            image {
               normal: "popup-bg.png";
               border: 34 34 34 34;
               border_scale: 1;
            }
            rel1.relative: 0.1 0.1;
            rel2.relative: 0.9 0.9;
         }
      }
      part { name: "base";
         type: SPACER;
	 scale: 1;
         description { state: "default" 0.0;
            rel1 {
               offset: 26 20;
               to: "pop";
            }
            rel2 {
               offset: -27 -21;
               to: "pop";
            }
         }
      }
      part { name: "elm.text.title";
         type: TEXT;
         scale: 1;
         description { state: "default" 0.0;
            align: 0.5 0.0;
            fixed: 1 1;
            min: 0 34;
            max: -1 34;
            color_class: "main";
            text {
               font: FN_B;
               size: 13;
               align: 0.0 0.5;
            }
            rel1 {
               relative: 1.0 0.0;
               offset: 3 0;
               to_x: "elm.swallow.icon";
               to_y: "base";
            }
            rel2 {
               relative: 0.0 1.0;
               offset: -4 -1;
               to_x: "elm.swallow.close";
               to_y: "base";
            }
         }
      }
      part { name: "elm.swallow.icon";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            align: 0.0 0.5;
            fixed: 1 0;
            min: 16 16;
            max: 16 16;
            rel1 {
               to_x: "base";
               to_y: "elm.text.title";
            }
            rel2.to_y: "elm.text.title";
         }
      }
      part { name: "elm.swallow.close";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            align: 1.0 0.5;
            fixed: 1 1;
            max: 16 16;
            aspect: 1.0 1.0;
            rel1 {
               to_x: "base";
               to_y: "elm.text.title";
            }
            rel2 {
               to_x: "base";
               to_y: "elm.text.title";
            }
         }
      }
      part { name: "separator.title";
         type: IMAGE;
         description { state: "default" 0.0;
            align: 0.5 0.0;
            min: 0 2;
            max: -1 2;
            image.normal: "separator-horizontal.png";
            rel1 {
               relative: 0.0 1.0;
               to_x: "base";
               to_y: "elm.text.title";
            }
            rel2.to_x: "base";
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            fixed: 1 0;
            rel1 {
               relative: 0.0 1.0;
               to_x: "base";
               to_y: "separator.title";
            }
            rel2 {
               relative: 1.0 0.0;
               to_x: "base";
               to_y: "separator.buttons";
            }
         }
      }
      part { name: "buttons.area";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            align: 0.5 1.0;
            min: 0 34;
            max: -1 34;
            rel1.to: "base";
            rel2.to: "base";
         }
      }
      part { name: "separator.buttons";
         type: IMAGE;
         description { state: "default" 0.0;
            align: 0.5 1.0;
            fixed: 1 1;
            min: 0 2;
            max: -1 2;
            image.normal: "separator-horizontal.png";
            rel1.to: "base";
            rel2 {
               relative: 1.0 0.0;
               to_x: "base";
               to_y: "buttons.area";
            }
         }
      }
      part { name: "eflete.swallow.btn_close";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            align: 1.0 0.5;
            fixed: 1 1;
            rel1 {
               relative: 1.0 0.5;
               to: "buttons.area";
            }
            rel2 {
               relative: 1.0 0.5;
               to: "buttons.area";
            }
         }
      }
      part { name: "eflete.swallow.btn_ok";
         type: SWALLOW;
         scale: 1;
         description { state: "default" 0.0;
            align: 1.0 1.0;
            fixed: 1 1;
            rel1.to: "eflete.swallow.btn_close";
            rel2 {
               relative: 0.0 1.0;
               offset: -7 -1;
               to: "eflete.swallow.btn_close";
            }
         }
      }
   }
   programs {
      program { name: "block_clicked";
         signal: "mouse,clicked,*";
         source: "block_events";
         action: SIGNAL_EMIT "elm,action,click" "elm";
         after: "deeper_block_color";
      }
      program { name: "deeper_block_color";
         action: STATE_SET "clicked" 0.00;
         target: "block_events";
         after: "default_block_color";
      }
      program { name: "default_block_color";
         action: STATE_SET "default" 0.00;
         transition: LINEAR 0.20000;
         target: "block_events";
      }
   }
}

