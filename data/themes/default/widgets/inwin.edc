group { name: "elm/win/inwin/splash";
   broadcast_signal: 0;
   parts {
      part { name: "block_events";
         type: RECT;
         scale: 1;
         description { state: "default" 0.0;
            color: 255 0 0 0;
         }
         description { state: "clicked" 0.0;
            inherit: "default" 0.0;
            color: 255 0 0 50;
         }
      }
      part { name: "bg";
         type: IMAGE;
         scale: 1;
         description { state: "default" 0.0;
            max: 0 0;
            image {
               normal: "popup-bg.png";
               border: 32 32 32 32;
               middle: DEFAULT;
            }
         }
         description { state: "visible" 0.0;
            min: 554 387;
            max: 554 387;
            image {
               normal: "popup-bg.png";
               border: 32 32 32 32;
               border_scale: 1;
               middle: DEFAULT;
            }
         }
      }
      part { name: "padding.rel1";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            align: 0 0;
            max: 27 27;
            rel1.to: "bg";
            rel2.to: "bg";
         }
      }
      part { name: "padding.rel2";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            align: 1.0 1.0;
            max: 27 27;
            rel1.to: "bg";
            rel2.to: "bg";
         }
      }
      part { name: "area.content";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            rel1 {
               relative: 1.0 1.0;
               to: "padding.rel1";
            }
            rel2 {
               relative: 0.0 0.0;
               to: "padding.rel2";
            }
         }
      }
      part { name: "area.info";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            align: 0.0 1.0;
            max: -1 30;
            rel1.to: "area.content";
            rel2.to: "area.content";
         }
      }
      part { name: "clipper.area.info";
         type: RECT;
         scale: 1;
         description { state: "default" 0.0;
            rel1.to: "area.info";
            rel2.to: "area.info";
            max: 0 0;
         }
         description { state: "visible" 0.0;
            inherit: "default" 0.0;
            max: -1 -1;
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         scale: 1;
         clip_to: "clipper.area.info";
         description { state: "default" 0.0;
            align: 1.0 0.5;
            max: 72 -1;
            fixed: 1 1;
            rel1.to: "area.info";
            rel1.relative: 1.0 0.0;
            rel2.to: "area.info";
         }
      }
      part { name: "label.info";
         type: TEXT;
         scale: 1;
         clip_to: "clipper.area.info";
         description { state: "default" 0.0;
            visible: 0;
            fixed: 1 1;
            rel1.to: "area.info";
            rel2 {
               relative: 0.0 1.0;
               to_x: "elm.swallow.content";
               to_y: "area.info";
            }
            text {
               font: FN;
               size: 12;
               align: 0.0 0.5;
            }
         }
         description { state: "visible" 0.0;
            inherit: "default" 0.0;
            visible: 1;
         }
      }
      part { name: "padding.rel2.anim";
         type: SPACER;
         scale: 1;
         description { state: "default" 0;
            align: 1.0 1.0;
            max: 7 7;
            rel1.to: "area.content";
            rel2 {
               relative: 1.0 0.0;
               to_x: "area.content";
               to_y: "area.info";
            }
         }
      }
      part { name: "splash.anim";
         type: IMAGE;
         scale: 1;
         description { state: "default" 0.0;
            max: 500 300;
            rel1.to: "area.content";
            rel2 {
               relative: 1.0 0.0;
               to_x: "area.content";
               to_y: "padding.rel2.anim";
